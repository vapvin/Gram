type User {
  id: ID! @id
  username: String!
  firstName: String!
  lastName: String!
  bio: String!
  posts: [Post!]!
  likes: [Like!]!
  comments: [Comments!]!
}

type Post {
  location: String!
  caption: String!
  user: User!
  files: [Files!]!
  likes: [Like!]!
  comments: [Comments!]!
}

type Like {
  user: User!
  post: Post!
}

type Comment {
  text: String!
  user: User!
  post: Post!
}

type File {
  url: String!
  post: Post!
}